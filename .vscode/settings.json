{
	"files.associations": {
		"*.erb": "erb",
		"*.h": "c",
		"*.c": "c",
		"*.cpp": "cpp",
		"*.hpp": "cpp",
		"cstdint": "cpp",
		"new": "cpp"
	},
	"github.copilot.enable": {
		"*": false,
		"plaintext": true,
		"markdown": true,
		"scminput": false,
		"yaml": true,
		"typescript": true,
		"typescriptreact": true,
		"rust": true
	}
}